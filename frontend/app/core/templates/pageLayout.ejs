<div class="page">
  <div class="hd">
    <nav class="navbar navbar-default"></nav>
    <div class="page-alerts"></div>
    <div class="page-header">
      <ul class="page-actions"></ul>
      <ul class="page-breadcrumbs"></ul>
    </div>
  </div>
  <div class="bd"></div>
  <div class="ft">
    <p>
      Walkner Xiconf <span id="<%- idPrefix %>-version"><%- version %></span><br>
      <a href="http://miracle.systems/">miracle.systems</a>
    </p>
  </div>
  <% if (showParentControls) { %>
  <div class="parentControls">
    <button id="<%- idPrefix %>-switchApps" class="btn btn-default" title="<%- t('core', 'parent:switchApps') %>"><i class="fa fa-window-restore"></i></button>
    <button id="<%- idPrefix %>-reboot" class="btn btn-default" title="<%- t('core', 'parent:reboot') %>"><i class="fa fa-refresh"></i></button>
    <button id="<%- idPrefix %>-shutdown" class="btn btn-default" title="<%- t('core', 'parent:shutdown') %>"><i class="fa fa-power-off"></i></button>
  </div>
  <% } %>
</div>
