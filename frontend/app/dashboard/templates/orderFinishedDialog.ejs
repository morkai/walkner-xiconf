<div>
  <p class="dashboard-orderFinishedDialog-label is-first"><%- t('dashboard', 'orderFinishedDialog:orderNo') %></p>
  <p class="dashboard-orderFinishedDialog-value"><%- order.no %></p>
  <div class="row">
    <div class="col-md-4">
      <p class="dashboard-orderFinishedDialog-label"><%- t('dashboard', 'orderFinishedDialog:quantity') %></p>
      <p class="dashboard-orderFinishedDialog-value"><%- order.quantity %></p>
    </div>
    <div class="col-md-4">
      <p class="dashboard-orderFinishedDialog-label"><%- t('dashboard', 'orderFinishedDialog:successCounter') %></p>
      <p class="dashboard-orderFinishedDialog-value"><%- order.successCounter %></p>
    </div>
    <div class="col-md-4">
      <p class="dashboard-orderFinishedDialog-label"><%- t('dashboard', 'orderFinishedDialog:failureCounter') %></p>
      <p class="dashboard-orderFinishedDialog-value"><%- order.failureCounter %></p>
    </div>
  </div>
  <div class="row">
    <div class="col-md-4">
      <p class="dashboard-orderFinishedDialog-label"><%- t('dashboard', 'orderFinishedDialog:startedAt') %></p>
      <p class="dashboard-orderFinishedDialog-value"><%- order.startedAt %></p>
    </div>
    <div class="col-md-4">
      <p class="dashboard-orderFinishedDialog-label"><%- t('dashboard', 'orderFinishedDialog:finishedAt') %></p>
      <p class="dashboard-orderFinishedDialog-value"><%- order.finishedAt %></p>
    </div>
    <div class="col-md-4">
      <p class="dashboard-orderFinishedDialog-label"><%- t('dashboard', 'orderFinishedDialog:duration') %></p>
      <p class="dashboard-orderFinishedDialog-value"><%- order.duration %></p>
    </div>
  </div>
  <button class="btn btn-lg btn-block btn-primary dialog-answer" type="button" autofocus data-answer="yes">
    <% if (remote) { %>
    <%- t('dashboard', 'orderFinishedDialog:close') %> <kbd>Esc</kbd>
    <% } else { %>
    <%- t('dashboard', 'orderFinishedDialog:yes') %> <kbd><%- resetOrderHotkey %></kbd>
    <% } %>
  </button>
</div>
