<form class="settings-form" method="post" action="/settings" autocomplete="off">
  <div class="list-group">
    <a data-tab="settings" class="list-group-item active" href="#settings?tab="><%- t('settings', 'form:settings') %></a>
    <a data-tab="remote" class="list-group-item" href="#settings?tab="><%- t('settings', 'form:remote') %></a>
    <a data-tab="mow" class="list-group-item" href="#settings?tab="><%- t('settings', 'form:mow') %></a>
    <a data-tab="gprs" class="list-group-item" href="#settings?tab="><%- t('settings', 'form:gprs') %></a>
    <a data-tab="lpt" class="list-group-item" href="#settings?tab="><%- t('settings', 'form:lpt') %></a>
    <a data-tab="sol" class="list-group-item" href="#settings?tab="><%- t('settings', 'form:sol') %></a>
    <a data-tab="testing" class="list-group-item" href="#settings?tab="><%- t('settings', 'form:testing') %></a>
    <a data-tab="glp2" class="list-group-item" href="#settings?tab="><%- t('settings', 'form:glp2') %></a>
    <a data-tab="fl" class="list-group-item" href="#settings?tab="><%- t('settings', 'form:fl') %></a>
    <a data-tab="hotkeys" class="list-group-item" href="#settings?tab="><%- t('settings', 'form:hotkeys') %></a>
    <a data-tab="license" class="list-group-item" href="#settings?tab="><%- t('settings', 'form:license') %></a>
  </div>
  <div class="panel panel-primary">
    <% include _settings.ejs %>
    <% include _remote.ejs %>
    <% include _mow.ejs %>
    <% include _gprs.ejs %>
    <% include _lpt.ejs %>
    <% include _sol.ejs %>
    <% include _testing.ejs %>
    <% include _glp2.ejs %>
    <% include _fl.ejs %>
    <% include _hotkeys.ejs %>
    <% include _license.ejs %>
    <div class="panel-footer">
      <input id="<%- idPrefix %>-password" class="form-control" name="password" type="password" required placeholder="<%- t('settings', 'form:password') %>"><input
        type="submit" class="btn btn-primary settings-save" value="<%- t('settings', 'form:submit') %>"><input
        type="button" class="btn btn-default settings-export" value="<%- t('settings', 'form:export') %>"><input
        type="button" class="btn btn-danger settings-restart" value="<%- t('settings', 'form:restart') %>">
    </div>
  </div>
</form>
