<div id="<%- idPrefix %>-scanner" class="panel-body">
  <div class="row">
    <div class="form-group col-md-4">
      <label for="<%- idPrefix %>-bgScanner" class="control-label"><%- t('settings', 'setting:bgScanner') %></label>
      <div>
        <label class="radio-inline">
          <input id="<%- idPrefix %>-bgScanner" name="bgScanner" type="radio" value="1" required> <%- t('settings', 'setting:bgScanner:on') %>
        </label>
        <label class="radio-inline">
          <input name="bgScanner" type="radio" value="0" required> <%- t('settings', 'setting:bgScanner:off') %>
        </label>
      </div>
    </div>
    <div class="form-group col-md-8">
      <label for="<%- idPrefix %>-bgScannerFilter" class="control-label"><%- t('settings', 'setting:bgScannerFilter') %></label>
      <input id="<%- idPrefix %>-bgScannerFilter" class="form-control" name="bgScannerFilter" type="text">
    </div>
  </div>
  <div class="row">
    <div class="form-group col-md-4">
      <label for="<%- idPrefix %>-bgScannerBeepGood" class="control-label"><%- t('settings', 'setting:bgScannerBeepGood') %></label>
      <select id="<%- idPrefix %>-bgScannerBeepGood" class="form-control" name="bgScannerBeepGood">
        <% include _bgScannerOptions.ejs %>
      </select>
    </div>
    <div class="form-group col-md-4">
      <label for="<%- idPrefix %>-bgScannerBeepBad" class="control-label"><%- t('settings', 'setting:bgScannerBeepBad') %></label>
      <select id="<%- idPrefix %>-bgScannerBeepBad" class="form-control" name="bgScannerBeepBad">
        <% include _bgScannerOptions.ejs %>
      </select>
    </div>
    <div class="form-group col-md-4">
      <label for="<%- idPrefix %>-bgScannerBeepDone" class="control-label"><%- t('settings', 'setting:bgScannerBeepDone') %></label>
      <select id="<%- idPrefix %>-bgScannerBeepDone" class="form-control" name="bgScannerBeepDone">
        <% include _bgScannerOptions.ejs %>
      </select>
    </div>
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-scannedValue" class="control-label"><%- t('settings', 'scanner:scannedValue') %></label>
    <textarea id="<%- idPrefix %>-scannedValue" class="form-control" name="scannedValue" rows="6"></textarea>
  </div>
  <div class="form-group text-center">
    <label><%- t('settings', 'scanner:defaults') %></label>
    <br>
    <img src="data:image/png;base64,">
  </div>
  <div class="form-group text-center" style="margin-top: 30px" data-bg-scanner="0">
    <label><%- t('settings', 'scanner:keyboard') %></label>
    <br>
    <img src="data:image/png;base64,">
  </div>
  <div class="form-group text-center" style="margin-top: 30px" data-bg-scanner="1">
    <label><%- t('settings', 'scanner:snapi') %></label>
    <br>
    <img src="data:image/png;base64,">
  </div>
  <div class="form-group text-center" style="margin-top: 30px" data-bg-scanner="0">
    <label><%- t('settings', 'scanner:enter') %></label>
    <br>
    <img src="data:image/png;base64,">
  </div>
</div>
